# AWS Configuration
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# AWS Cognito Configuration
COGNITO_CLIENT_ID=your_cognito_client_id
COGNITO_CLIENT_SECRET=your_cognito_client_secret
COGNITO_USER_POOL_ID=your_user_pool_id

# Server Configuration
PORT=3001

# S3 bucket used for presigned uploads. Set to your bucket name, e.g. my-app-uploads
S3_BUCKET_NAME=your_s3_bucket_name

# AWS AI Services (Optional - set to 'true' to enable)
TEXTRACT_ENABLED=true
BEDROCK_ENABLED=true
BEDROCK_MODEL=anthropic.claude-3-opus-20240229-v1:0

# Auto-update Cognito client settings (Optional)
COGNITO_AUTO_UPDATE_CLIENT=false